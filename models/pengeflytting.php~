<?php
class Kaffeflytting extends AppModel {
  var $useTable = 'kaffeflytting'; 
  var $primaryKey = 'nummer';
  var $belongsTo = array(
		      'Fralager' => array(
					  'className' => 'Kaffelager',
					  'foreignKey' => 'fra'
					  ),
		      'Tillager' => array(
					  'className' => 'Kaffelager',
					  'foreignKey' => 'til'
					  ),
		      'Kaffetype' => array(
					   'className' => 'Kaffepris',
					  'foreignKey' => 'type'
					  )
			 );
  }
?>